{
  "name": "node-js-applications",
  "version": "1.0.0",
  "description": "Creating a Node.js demo repo with notes, examples, and applications.",
  "directories": {
    "examples": "examples",
    "applications": "applications",
    "taking-notes": "taking-notes"
  },
  "scripts": {
    "app:notes": "node applications/taking-notes/main.js",
    "inspect:notes": "node inspect applications/taking-notes/main.js",
    "app:task": "nodemon applications/task/main.js",
    "inspect:task": "node inspect applications/task/main.js",
    "app:weather": "node applications/weather/src/main.js",
    "inspect:weather": "node inspect applications/weather/main.js",
    "serve:weather": "nodemon applications/weather/build_scripts/weather-server.js -e js,ejs",
    "example:append": "node examples/appendFile/append-message.js",
    "example:exporting": "node examples/exporting/importing.js",
    "example:json": "node examples/json/json.js",
    "example:raw-http": "node examples/raw-http/raw-http.js",
    "example:web-server": "nodemon examples/web-server/express.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/wdonray/node.js-applications.git"
  },
  "author": "Donray Williams",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/wdonray/node.js-applications/issues"
  },
  "homepage": "https://github.com/wdonray/node.js-applications#readme",
  "type": "module",
  "dependencies": {
    "axios": "^1.2.0",
    "chalk": "^5.1.2",
    "dotenv": "^16.0.3",
    "ejs": "^3.1.8",
    "express": "^4.18.2",
    "validator": "^13.7.0",
    "yargs": "^17.6.2"
  }
}
